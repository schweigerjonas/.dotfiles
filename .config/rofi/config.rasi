configuration {
  modes: "run,window,combi";
  icon-theme: "Oranchelo";
  show-icons: true;
	terminal: "rofi-sensible-terminal";
  drun-display-format: "{icon} {name}";
  location: 0;
  disable-history: false;
  hide-scrollbar: true;
  display-combi: "All";
  display-run: "Run";
  display-window: "Window";
  sidebar-mode: true;
}

@theme "catppuccin-mocha"
