configuration {
  modes: "run,window,combi";
	terminal: "rofi-sensible-terminal";
  drun-display-format: "{name}";
  location: 0;
  disable-history: false;
  hide-scrollbar: true;
  display-combi: "All";
  display-run: "Run";
  display-window: "Window";
  sidebar-mode: true;
}

@theme "iceberg-contrast"
